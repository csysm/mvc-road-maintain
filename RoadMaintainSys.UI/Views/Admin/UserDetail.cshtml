@model RoadMaintainSys.Entities.User

@{
    ViewData["Title"] = "用户详情";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}
@section AdminName {
    欢迎您:@ViewBag.AdminName
}

<h1>用户详情</h1>
<hr/>
<div>
    <dl class="row">
        <dt class = "col-sm-2">
            用户账号:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserId)
        </dd>
        <dt class = "col-sm-2">
            密码:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Password)
        </dd>
        <dt class = "col-sm-2">
            用户名:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserName)
        </dd>
        <dt class = "col-sm-2">
            创建时间:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CreateTime)
        </dd>
    </dl>
</div>

<p style="position:absolute;bottom:50px;">
    <a asp-action="UserList">返回用户列表</a>
</p>